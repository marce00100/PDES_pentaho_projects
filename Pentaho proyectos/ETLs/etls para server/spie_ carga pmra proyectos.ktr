    <sql>SELECT&#xd;&#xa;p.id AS id_p,&#xd;&#xa;p.cod_p,&#xd;&#xa;p.nombre AS nombre_p,&#xd;&#xa;p.titulo AS desc_p,&#xd;&#xa;m.id AS id_m,&#xd;&#xa;m.cod_m,&#xd;&#xa;m.nombre AS nombre_m,&#xd;&#xa;m.descripcion AS desc_m,&#xd;&#xa;r.id AS id_r,&#xd;&#xa;r.cod_r,&#xd;&#xa;r.nombre AS nombre_r,&#xd;&#xa;r.descripcion AS desc_r,&#xd;&#xa;a.id AS id_a,&#xd;&#xa;a.cod_a,&#xd;&#xa;a.nombre AS nombre_a,&#xd;&#xa;a.descripcion AS desc_a,&#xd;&#xa;concat&#x28;p.cod_p,&#x27;.&#x27;,m.cod_m,&#x27;.&#x27;,r.cod_r,&#x27;.&#x27;,a.cod_a&#x29; AS codigo,&#xd;&#xa;IF &#x28;tc.id &#x3d; 4,  &#x27;Accion&#x27; , tc.tipo &#x29; AS categoria,&#xd;&#xa;ai.nombre AS producto_proyecto,&#xd;&#xa;ai.sisin,&#xd;&#xa;pr.codigo AS cod_demanda,&#xd;&#xa;pr.total_costo,&#xd;&#xa;e.nombre AS ejecutor_sp,&#xd;&#xa;e.sigla,&#xd;&#xa;se.nombre AS entidad_responsable,&#xd;&#xa;dep.departamento,&#xd;&#xa;prov.provincia,&#xd;&#xa;mun.municipio,&#xd;&#xa;com.comunidad,&#xd;&#xa;r.meta&#xd;&#xa;FROM&#xd;&#xa;pilar AS p&#xd;&#xa;JOIN meta AS m ON p.id &#x3d; m.pilar&#xd;&#xa;JOIN resultado AS r ON m.id &#x3d; r.meta&#xd;&#xa;JOIN acciones AS a ON r.id &#x3d; a.resultado &#xd;&#xa;LEFT JOIN accion AS ai ON ai.accion &#x3d; a.id AND ai.estado &#x3d; 1 AND ai.tipo &#x3c;&#x3e; 1&#xd;&#xa;LEFT JOIN tipocategoria AS tc ON tc.id &#x3d; ai.tipo&#xd;&#xa;LEFT JOIN entidad AS e ON ai.entidad &#x3d; e.id&#xd;&#xa;LEFT JOIN proyectos AS pr ON ai.proyecto &#x3d; pr.id AND pr.estado &#x3d; 1&#xd;&#xa;LEFT JOIN territorializacion AS tr ON tr.accion &#x3d; ai.id&#xd;&#xa;LEFT JOIN municipios AS mun ON mun.id &#x3d; tr.municipio&#xd;&#xa;LEFT JOIN provincias AS prov ON prov.id &#x3d; tr.provincia&#xd;&#xa;LEFT JOIN comunidades AS com ON com.id &#x3d; tr.comunidad&#xd;&#xa;LEFT JOIN departamentos AS dep ON tr.id &#x3d; dep.id&#xd;&#xa;LEFT JOIN entidad_responsable AS er ON er.accion &#x3d; ai.id&#xd;&#xa;LEFT JOIN sub_entidad AS se ON er.entidad_responsable &#x3d; se.id</sql>
