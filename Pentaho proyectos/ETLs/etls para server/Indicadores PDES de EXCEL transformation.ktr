    <quoteString>N</quoteString>
    <sql>&#xd;&#xa;-- Obtiene los numeros de los indicador_nombre que empiezan co R o con PC-R , toma los primero 10 caracteres para tener un margen, &#xd;&#xa;-- &#x2a; no exiten concidencias en eso 10 caracteres con otros indicaores que si empiazan con R, ejemplo &#x22;Realizar los ..&#x22;&#xd;&#xa;-- &#x2a; los indicadores_nombre que empizan realmente con R o con PC y no tienen un numero asociado en esos pirmeros 10 caracteres seran llenados con null&#xd;&#xa;-- &#x2a; en la segunda sentencia query update se cambian los null por 0&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;UPDATE tmp_pmr_indicadores &#xd;&#xa;&#xd;&#xa;SET cod_r_asociado &#x3d; substring &#x28;substring&#x28;indicador_nombre,1,10&#x29; from &#x27;&#x5b;0-9&#x5d;&#x2b;&#x27;&#x29;&#x3a;&#x3a;int&#xd;&#xa;WHERE indicador_nombre ilike &#x27;R&#x25;&#x27; or indicador_nombre ilike &#x27;PC&#x25;&#x27; &#x3b;&#xd;&#xa;&#xd;&#xa;UPDATE tmp_pmr_indicadores  set cod_r_asociado &#x3d; 0 where coalesce&#x28;cod_r_asociado, 0&#x29; &#x3d; 0</sql>
    <set_params>N</set_params>
