    <sql>-- Para llenar la tabla resultado_indicadores se tienen dos tipos de relaciones, &#xd;&#xa;-- los indicadores que son directos para un resultado y los indicadores que son heredados de otro resultado&#xd;&#xa;&#xd;&#xa;--  							cod_r	&#x7c; nombre_indicador	&#x7c; cod_r_asociado	&#x7c; nombre_indicador_modificado&#xd;&#xa;------							--------------------------------------------------------------------------------&#xd;&#xa;-- ejemplo indicador directo	666		&#x7c;indicador hhh		&#x7c;	0				&#x7c;	indicador hhh&#xd;&#xa;-- ej. indicador heredado		777		&#x7c;R. 555. indicador k&#x7c;	555				&#x7c;	indicador k&#xd;&#xa;&#xd;&#xa;-- En el caso de indicador directo&#x3a; se relacionan el resultado cod_r con el indicador que coindica con nombre_indicador_modificado y la columna indicador_directo &#x3d; true&#xd;&#xa;-- En el caso de Indicador heredado&#x3a;	se relaciona el resultado cod_r_asociado con el indicador que coincida con nombre_indicador_modificado&#x3b; indicador-directo &#x3d; true&#xd;&#xa;--										se relaciona el resultado cod_r con el indicador que coindica con nombre_indicador_modificado&#x3b; la columna indicador_directo &#x3d; false.&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;-- el campo punto_medicion se lo asocia con el campo indicador_etapa&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;-- Se pone en 1 la autosecuencia, si da error comentar esta linea no es importane&#xd;&#xa;&#xd;&#xa;ALTER SEQUENCE resultado_indicadores_id_resultado_indicador_seq RESTART WITH 1&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;-------------------- Resultado con un indicador directo.   Los que tienen cod_r_heredado &#x3d; 0&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;INSERT INTO resultado_indicadores &#xd;&#xa;&#x28;id_resultado, id_indicador, punto_medicion, indicador_directo&#x29; &#xd;&#xa;&#xd;&#xa;SELECT DISTINCT &#xd;&#xa;-- t.cod_r as tcod_r, t.indicador_nombre, t.indicador_nombre_modificado, &#xd;&#xa;--t.cod_r_asociado, r.cod_r,&#xd;&#xa;r.id,   i.id_indicador, t.indicador_etapa, true&#xd;&#xa;--, i.nombre &#xd;&#xa;from tmp_pmr_indicadores t, resultados r, indicadores i   &#xd;&#xa;where t.cod_r &#x3d; r.cod_r and t.indicador_nombre_modificado &#x3d;  i.nombre &#xd;&#xa;and cod_r_asociado &#x3d; 0  &#xd;&#xa;order by r.id,   i.id_indicador&#x3b;&#xd;&#xa;&#xd;&#xa;-------------------- Resultados asociados  a un indicador heredado por otro resultado &#xd;&#xa;---         primero la relacion entre cod_r_asociado y nombre_indicador_modificado, &#x28;indicadores directos con cod_r_asociado&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;INSERT INTO resultado_indicadores &#xd;&#xa;&#x28;id_resultado, id_indicador, punto_medicion, indicador_directo&#x29; &#xd;&#xa;&#xd;&#xa;SELECT DISTINCT  &#xd;&#xa;--t.cod_r_asociado, t.indicador_nombre_modificado, r.cod_r, i.nombre, &#xd;&#xa;r.id, i.id_indicador, null, true &#xd;&#xa;from tmp_pmr_indicadores t, resultados r, indicadores i   &#xd;&#xa;where t.cod_r_asociado &#x3d; r.cod_r and t.indicador_nombre_modificado &#x3d; i.nombre &#xd;&#xa;and cod_r_asociado &#x3e; 0&#x3b;&#xd;&#xa;&#xd;&#xa; &#xd;&#xa;&#xd;&#xa;---           segundo relacion entre cod_r y nombre_indicador_modificado &#x28;indcadore heredados&#xd;&#xa;&#xd;&#xa;INSERT INTO resultado_indicadores &#xd;&#xa;&#x28;id_resultado, id_indicador, punto_medicion, indicador_directo&#x29; &#xd;&#xa;&#xd;&#xa;SELECT DISTINCT&#xd;&#xa;--t.cod_r, t.indicador_nombre_modificado, r.cod_r, i.nombre, &#xd;&#xa;r.id, i.id_indicador, t.indicador_etapa, false &#xd;&#xa;from tmp_pmr_indicadores t, resultados r, indicadores i   &#xd;&#xa;where t.cod_r &#x3d; r.cod_r and t.indicador_nombre_modificado &#x3d; i.nombre &#xd;&#xa;and cod_r_asociado &#x3e; 0&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
